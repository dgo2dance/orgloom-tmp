} 

.weui-cell-text:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid rgba(0,0,0,.1);
  color: rgba(0,0,0,.1);
  left: 16px;
  
  }

  .selector{
    height: 60rpx;
    width: 100%;

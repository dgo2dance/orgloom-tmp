/*科室排班表  */

@import "../../utils/weui.wxss";
@import "../../utils/index.wxss";


.con_input{

  margin: 100rpx auto;
display: flex;
justify-content: center;
flex-direction: column;

}

.getButton{
  width: 70%;
margin: 0 auto;

}
.table{
  display: inline-flex;
  flex-direction: column;
  border: 1rpx solid rgba(218, 217, 217, 1);
  border-bottom: 0;  
}

.scrollClass {
  display: flex;
  width: 100%;
  white-space: nowrap;
  margin-top: 23px;
  height: 100%;
  background-color: white;
}

.table_header {
  display: inline-flex;
}

.th {
  display: flex;
  flex-direction: column;
  width: 89rpx;
  height: 89rpx;
  background: rgba(241, 252, 255, 1);
  border-right: 1rpx solid rgba(218, 217, 217, 1); 
  border-bottom: 1rpx solid rgba(218, 217, 217, 1); 
  justify-content: center;
  align-items: center;
  overflow-x: auto;
}
.cell_label{
  font-size: 26rpx;
  color: rgba(74, 74, 74, 1);
}
.cell_date_label{
  font-size: 20rpx;
  color: rgba(74, 74, 74, 1);
}
.table_main {
  display: inline-flex;
  flex-direction: row;
}
.right-item{
  display: flex;
  flex-direction: row;
}
.td {
  display: flex;
  flex-direction: column;
  width: 89rpx;
  /* height: 90rpx; */
  background: white;
  justify-content: center;
  align-items: center;
  border: 1rpx solid rgba(200, 200, 200, 0.5);
  border-top: 0;
  border-left:0; 
}

.table_Text_class {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60rpx;
  font-size: 30rpx;
  color: rgba(55, 134, 244, 1);
  width: 100%;
  word-break: normal;
  border-bottom: 1rpx solid rgba(218, 217, 217, 1);
}
.table_Text_last_class{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60rpx;
  font-size: 30rpx;
  color: rgba(55, 134, 244, 1);
  width: 100%;
  word-break: normal;

}
.pop-row-first{
margin-right: 50px;
height: 120rpx;
line-height: 10rpx;

}
.header-cell{
  text-align:center;
}


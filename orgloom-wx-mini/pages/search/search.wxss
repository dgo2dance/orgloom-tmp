.top_contain {
      width: 100%;
      display: flex;
      justify-content: space-between;
      padding: 20rpx 30rpx;
      box-sizing: border-box;
    /*  background: #fbbd08; */
      background: #37a3fd;
      height: 106rpx;
      align-items: center;
}

.search_box {
      width: 80%;
      height: 88rpx;
      border-radius: 15rpx 0 0 15rpx;
      background: #fff;
      display: flex;
      align-items: center;
}

.search_box image {
      padding-left: 30rpx;
      width: 32rpx;
      height: 32rpx;
}

.search_box input {
      padding-left: 10rpx;
      font-size: 28rpx;
      letter-spacing: 2rpx;
      color: rgb(167, 167, 167);
      background: #fff;
      height: 66rpx;
      width: 500rpx;
}

.search {
      width: 20%;
      height: 88rpx;
      display: flex;
      justify-content: center;
      align-items: center;
      background: rgb(0, 0, 0);
      color: #fff;
      font-size: 28rpx;
      border-radius: 0 16rpx 16rpx 0;
      letter-spacing: 4rpx;
}

.contain {
      width: 100%;
      padding: 0 30rpx;
      display: flex;
      box-sizing: border-box;
      flex-direction: column;
}

.a {
      display: flex;
      align-items: center;
      border-bottom: 1rpx solid #eee;
      width: 100%;
      padding: 20rpx 0;
}

.a image {
      width: 36rpx;
      height: 36rpx;
}

.a view {
      font-size: 30rpx;
      letter-spacing: 2rpx;
      padding-left: 20rpx;
}

.hisitem {
      width: 100%;
      font-size: 28rpx;
      letter-spacing: 2rpx;
      color: #919191;
      padding: 16rpx 0;
}

.list_box {
      width: 100%;
      display: flex;
      align-items: center;
      padding: 20rpx 0;
      box-sizing: border-box;
      margin-top: 10rpx;
}

.list_poster {
      width: 200rpx;
      height: 200rpx;
}

.list_content {
      height: 200rpx;
      width: calc(100% - 230rpx);
      display: flex;
      flex-direction: column;
      justify-content: space-between;
      padding: 0 10rpx;
      box-sizing: border-box;
}

.list_word {
      display: flex;
      flex-direction: column;
      width: 100%;
      box-sizing: border-box;
}

.list_title {
      width: 100%;
      font-size: 32rpx;
      letter-spacing: 3rpx;
      line-height: 45rpx;
}

.list_author {
      padding-top: 10rpx;
      width: 100%;
      font-size: 27rpx;
      letter-spacing: 3rpx;
      line-height: 40rpx;
      color: rgb(129, 129, 129);
}

.list_between {
      width: 100%;
      display: flex;
      justify-content: space-between;
      align-items: flex-end;
      border-bottom: 1rpx solid #eee;
      padding-bottom: 10rpx;
}

.list_price {
      font-size: 36rpx;
      color: rgb(255, 0, 0);
}

.list_time {
      font-size: 28rpx;
      letter-spacing: 2rpx;
      color: rgb(129, 129, 129);
}


.nocontent {
      width: 100%;
    /*  height: 400rpx; */
      margin-bottom: 40rpx;
      display: flex;
      justify-content: center;
      align-items: center;
      flex-direction: column;
      box-sizing: border-box;
}

.nocontent image {
      padding-top: 80rpx;
      width: 100rpx;
      height: 100rpx;
}
.blank_text{
      padding-top: 40rpx;
      font-size: 28rpx;
      letter-spacing: 2rpx;
      color: #c6c6c8;
}
.nosearch{ 
      width: 100%;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 28rpx;
      height: 100rpx;
      color: #919191;
      letter-spacing: 4rpx;
}


.select-content-container{
      width: 100%;  
      display: flex;
      flex-direction: column;
      background-color: white;
    }
    
    .select-content-container .tab-title-container{
      background-color: white;  
      height: 80rpx;
      width: 100%; 
      display: flex;
      flex-direction: row;
      align-items: center;
      justify-content: space-between;
   /*   border-bottom: 1rpx solid #EEEEEE; */
    }
    
    .tab-title-container .title-button{
      display: flex;
      flex-direction: row;
      justify-content: center;
      width: 25%;
      font-size: 30rpx;  
      color:#666;
    }
    


.title-button .active{
      color:RGB(254,74,78); 
    }
    
    .title-button .up{
      /*margin-left: -5rpx;*/
    }
    .title-button .down{
      /*margin-left: -10rpx;*/
    }
    
    .select-content-container .select-stock-container{
      width: 100%;
      height: 90rpx;  
      background-color: white;   
      border-bottom: 1rpx solid #EEEEEE; 
      display: flex;
      flex-direction: row;
      align-items: center;
    }
    
    .select-stock-container.up{
      background: linear-gradient(to right, white , RGB(255,221,222));
    }
    .select-stock-container.down{
   /*   background: linear-gradient(to right, white , RGB(213,244,210)); */
    }
    
    .select-stock-container .stock-name-container{
      width: 25%;  
      display: flex;
      flex-direction: column;  
      text-align: center;
    }
    .stock-name-container .stock-name{  
      font-size: 35rpx;  
      color:rgb(141, 140, 140);
    }
    
    .stock-name-container .stock-code{
      font-size: 25rpx;  
      color: RGB(196, 196, 196);
    }
    
    .select-stock-container .select-stock-price{
      width: 25%;
      font-size: 40rpx;  
      text-align: center;
    }
    
    .select-stock-price.up{
      color: RGB(237, 85, 83);
    }
    
    .select-stock-price.down{
  /*    color: RGB(80, 158, 95); */
      color: rgb(141, 140, 140);
      font-size: 35rpx;
    }